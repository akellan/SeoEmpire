<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="SeoEmpire.Common" assembly="SeoEmpire">
<class name="News" table="test_news">
  <id name="Id" unsaved-value="0">
    <column name="NId" not-null="true" />
    <generator class="identity"/>
  </id>
  <property name="Title" column="NTitle" type="String" length="255" />
  <property name="Date" column="NDate" type="DateTime" />
  <property name="Body" column="NBody" />
  <property name="Image" column="NImage" type="BinaryBlob" />
</class>
</hibernate-mapping>

